{
  "version": 2,
  "public": true,
  "name": "spar-ng",
  "builds": [
    {
      "src": "package.json", "use": "@now/static-build",
      "config": {
        "distDir": "dist/spar-ng-test/",
        "useBuildUtils": "@now/build-utils@canary"
      }
    },
    { "src": "src/app/server/index.ts", "use": "@now/node-server"}
  ],
  "routes": [
    { "src": "/", "methods": ["GET"], "dest": "/index.html"},
    { "src": "/", "methods": ["GET"] ,"dest": "/src/app/server/index.ts" },
    { "src": "/users", "methods": ["GET"] ,"dest": "/src/app/server/index.ts" },
    { "src": "/login", "methods": ["POST"] ,"dest": "/src/app/server/index.ts" },
    { "src": "/(.*\\..*)","dest": "/$1" }
  ]
}
