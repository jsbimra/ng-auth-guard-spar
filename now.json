{
  "version": 2,
  "public": true,
  "name": "spar-ng",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "(.*\\..*))",
      "dest": "$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}